<?xml version="1.0"?>
<Style xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <StyleName>Kids</StyleName>
  <StylePath>Kids</StylePath>
  <StyleVersion>1.1</StyleVersion>
  <QuestionStylesheetPath>Kids\question.xsl</QuestionStylesheetPath>
  <AnswerStylesheetPath>Kids\answer.xsl</AnswerStylesheetPath>
  <StyledControls>
    <!-- Statistics Control = Toolbar-->
    <item>
      <key>gradientPanelStatisticsPanel</key>
      <value>
        <ControlSettings>
          <Gradient Type="Gradient">
            <PanelGradient>
              <Colors>
                <Color>148,203,0</Color>
                <Color>148,203,0</Color>
              </Colors>
              <Positions>
                <Position>0</Position>
                <Position>1</Position>
              </Positions>
              <Direction>Horizontal</Direction>
            </PanelGradient>
          </Gradient>
          <BackColor>148,203,0</BackColor>
        </ControlSettings>
      </value>
    </item>
    <item>
      <key>statisticsPanel</key>
      <value>
        <ControlSettings>
          <BackColor>148,203,0</BackColor>
          <ForeColorTitle Type="Color">White</ForeColorTitle>
          <ForeColorValue Type="Color">White</ForeColorValue>
          <FontTitle Type="Font">Tahoma, 8.25pt, style=Bold</FontTitle>
          <FontValue Type="Font">Tahoma, 9.75pt, style=Bold</FontValue>
        </ControlSettings>
      </value>
    </item>
    <!-- Statistics Control = Toolbar END-->
    <!-- Answer Control -->
    <item>
      <key>answerPanel</key>
      <value>
        <ControlSettings>
          <BackColor>Transparent</BackColor>
        </ControlSettings>
      </value>
    </item>
    <item>
      <key>panelMultiPane</key>
      <value>
        <ControlSettings>
          <BackColor>255,255,255</BackColor>
          <BorderStyle Type="BorderStyle">None</BorderStyle>
        </ControlSettings>
      </value>
    </item>
    <item>
      <key>multiPaneControlMain</key>
      <value>
        <ControlSettings>
          <BackColor>Transparent</BackColor>
          <BorderStyle Type="BorderStyle">None</BorderStyle>
        </ControlSettings>
      </value>
    </item>
    <item>
      <key>gradientPanelAnswer</key>
      <value>
        <ControlSettings>
          <BackColor>Transparent</BackColor>
        </ControlSettings>
      </value>
    </item>
    <item>
      <key>labelAnswer</key>
      <value>
        <ControlSettings>
          <ForeColor>255,255,255</ForeColor>
          <BackColor>Transparent</BackColor>
          <Font>Tahoma, 9.75pt, style=Bold</Font>
        </ControlSettings>
      </value>
    </item>
    <item>
      <key>mLifterTextBox</key>
      <value>
        <ControlSettings>
          <ForeColor>Black</ForeColor>
          <TippForeColor Type="Color">Silver</TippForeColor>
          <Font>Tahoma, 16pt, style=Bold</Font>
          <BorderStyle Type ="BorderStyle">FixedSingle</BorderStyle>
        </ControlSettings>
      </value>
    </item>
    <!-- Answer Control END-->
    <!-- Question Control -->
    <item>
      <key>questionPanel</key>
      <value>
        <ControlSettings>
          <BackColor>Transparent</BackColor>
        </ControlSettings>
      </value>
    </item>
    <item>
      <key>gradientPanelQuestion</key>
      <value>
        <ControlSettings>
        </ControlSettings>
      </value>
    </item>
    <item>
      <key>labelQuestion</key>
      <value>
        <ControlSettings>
          <ForeColor>255,255,255</ForeColor>
          <BackColor>Transparent</BackColor>
          <Font>Tahoma, 9.75pt, style=Bold</Font>
        </ControlSettings>
      </value>
    </item>
