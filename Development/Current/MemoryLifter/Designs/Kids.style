    <!-- Answer/Question Container-->
    <item>
      <key>splitContainerVertical</key>
      <value>
        <ControlSettings>
          <BackColor>246,168,0</BackColor>
        </ControlSettings>
      </value>
    </item>
    <item>
      <key>panelLearningWindowMiddle</key>
      <value>
        <ControlSettings>
          <BackColor>246,168,0</BackColor>
        </ControlSettings>
      </value>
    </item>
    <item>
      <key>gradientPanelMain</key>
      <value>
        <ControlSettings>
          <BackColor>Transparent</BackColor>
          <Gradient Type="Gradient">
            <PanelGradient>
              <Colors>
                <Color>246,168,0</Color>
                <Color>246,168,0</Color>
              </Colors>
              <Positions>
                <Position>0</Position>
                <Position>1</Position>
              </Positions>
              <Direction>Horizontal</Direction>
            </PanelGradient>
          </Gradient>
        </ControlSettings>
      </value>
    </item>
    <!-- Answer/Question Container END-->
    <!-- Multiple Choice-->
    <item>
      <key>multipleChoice</key>
      <value>
        <ControlSettings>
          <ForeColor>23,112,156</ForeColor>
          <BackColor>Transparent</BackColor>
          <Font>Arial, 14.25pt, style=Bold</Font>
		  <ImageChecked Type="Image">checkmark.png</ImageChecked>
        </ControlSettings>
      </value>
    </item>
    <item>
      <key>multiPanePageMainMultipleChoice</key>
      <value>
        <ControlSettings>
          <BackColor>Transparent</BackColor>
        </ControlSettings>
      </value>
    </item>
    <!-- Multiple Choice END-->
    <!-- Don't aks again-->
    <item>
      <key>dockingButtonDontAskAgain</key>
      <value>
        <ControlSettings>
			<BackColor>246,168,0</BackColor>
			<ForeColor>255, 255, 255</ForeColor>
          <BorderStyle Type="BorderStyle">None</BorderStyle>
        </ControlSettings>
      </value>
    </item>
    <!-- Don't aks again END-->
