      <key>gradientPanelButtonControlBackground</key>
      <value>
        <ControlSettings>
          <BackColor>148,203,0</BackColor>
          <Gradient Type="Gradient">
            <PanelGradient>
              <Colors>
                <Color>148,203,0</Color>
                <Color>255,255,255</Color>
                <Color>148,203,0</Color>
              </Colors>
              <Positions>
                <Position>0</Position>
                <Position>0.5</Position>
                <Position>1</Position>
              </Positions>
              <Direction>Horizontal</Direction>
            </PanelGradient>
          </Gradient>
        </ControlSettings>
      </value>
    </item>
    <!-- Button Control END-->
    <!-- Card Stack -->
    <item>
      <key>dockingButtonStack</key>
      <value>
        <ControlSettings>
          <BackColor>246,168,0</BackColor>
          <ForeColor>255, 255, 255</ForeColor>
        </ControlSettings>
      </value>
    </item>
    <item>
      <key>stackFlow</key>
      <value>
        <ControlSettings>
          <BackColor>255,255,255</BackColor>
          <ForeColor>Black</ForeColor>
		  <BackgroundImage Type="Image">Stack.png</BackgroundImage>
		  <BackgroundImageLayout Type="ImageLayout">Stretch</BackgroundImageLayout>
          <Font>Tahoma, 9pt, style=Bold</Font>
        </ControlSettings>
      </value>
    </item>
    <item>
      <key>stackFlow_Wrong</key>
      <value>
        <ControlSettings>
          <BackColor>255,255,255</BackColor>
        </ControlSettings>
      </value>
    </item>
    <item>
      <key>tableLayoutPanel</key>
      <value>
        <ControlSettings>
          <BackColor>Transparent</BackColor>
        </ControlSettings>
      </value>
    </item>
