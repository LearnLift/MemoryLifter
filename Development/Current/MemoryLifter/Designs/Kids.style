  <PrintStyles>
    <!-- DEFAULT -->
    <StyleInfo>
      <Path>System\Print\default.xsl</Path>
      <Name>Print All</Name>
      <ResourceID>PRINT_STYLE_PRINTALL</ResourceID>
    </StyleInfo>
    <!-- WITHOUT MEDIA -->
    <StyleInfo>
      <Path>System\Print\withoutMedia.xsl</Path>
      <Name>Print without Media</Name>
      <ResourceID>PRINT_STYLE_WITHOUTMEDIA</ResourceID>
    </StyleInfo>
  </PrintStyles>
